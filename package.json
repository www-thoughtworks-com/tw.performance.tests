{
  "name": "perf",
  "description": "tw.com website performance tests",
  "dependencies": {
    "async": "^1.5.2",
    "http-benchmark": "0.2.4",
    "mkdirp": "^0.5.1"
  },
  "scripts": {
    "report": "node consistent_load.js",
    "syncDown": "sh sync --down",
    "syncUp": "sh sync --up",
    "graph": "node graph_results.js",
    "syncProfiles": "sh sync-content tw.showcase.content tw.perf.content profiles",
    "syncArticles": "sh sync-content tw.showcase.content tw.perf.content articles",
    "syncNews": "sh sync-content tw.showcase.content tw.perf.content news",
    "all": "npm run syncDown && npm run report && npm run graph && npm run syncUp"
  }
}
